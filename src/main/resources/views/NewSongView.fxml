<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="260.0" prefWidth="466.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.mytunes.GUI.Controller.NewSongController">
   <children>
      <Button fx:id="btnSave" layoutX="397.0" layoutY="213.0" mnemonicParsing="false" onAction="#onCreate" text="Save" />
      <Button fx:id="btnCancel" layoutX="334.0" layoutY="213.0" mnemonicParsing="false" onAction="#onCancelButtonPressed" text="Cancel" />
      <ComboBox fx:id="cbbCategory" editable="true" layoutX="83.0" layoutY="96.0" prefWidth="150.0" style="-fx-background-color: white; -fx-border-color: gray; -fx-border-radius: 5; -fx-background-radius: 5;" />
      <TextField fx:id="txtTime" editable="false" layoutX="84.0" layoutY="131.0" prefHeight="25.0" prefWidth="149.0" style="-fx-background-color: light_gray; -fx-border-color: black; -fx-border-radius: 5; -fx-background-radius: 5;" />
      <TextField fx:id="txtFile" editable="false" layoutX="84.0" layoutY="167.0" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="25.0" prefWidth="206.0" style="-fx-background-color: light_gray; -fx-border-color: black; -fx-border-radius: 5; -fx-background-radius: 5;" />
      <TextField fx:id="txtArtist" layoutX="84.0" layoutY="61.0" prefHeight="25.0" prefWidth="282.0" style="-fx-background-color: white; -fx-border-color: gray; -fx-border-radius: 5; -fx-background-radius: 5;" />
      <TextField fx:id="txtTitle" layoutX="84.0" layoutY="26.0" prefHeight="25.0" prefWidth="282.0" style="-fx-background-color: White; -fx-border-color: gray; -fx-border-radius: 5; -fx-background-radius: 5;" />
      <Label alignment="CENTER_RIGHT" layoutY="100.0" prefHeight="17.0" prefWidth="72.0" text="Category:" textAlignment="RIGHT" />
      <Label alignment="CENTER_RIGHT" layoutY="135.0" prefHeight="17.0" prefWidth="72.0" text="Time:" textAlignment="RIGHT" />
      <Label alignment="CENTER_RIGHT" layoutY="171.0" prefHeight="17.0" prefWidth="72.0" text="Selected File:" textAlignment="RIGHT" />
      <Label alignment="CENTER_RIGHT" layoutY="65.0" prefHeight="17.0" prefWidth="72.0" text="Artist:" textAlignment="RIGHT" />
      <Label alignment="CENTER_RIGHT" layoutY="30.0" prefHeight="17.0" prefWidth="72.0" text="Title:" textAlignment="RIGHT" />
      <Button fx:id="btnFilePicker" layoutX="289.0" layoutY="167.0" mnemonicParsing="false" onAction="#onFilePickerPressed" text="Choose File" />
   </children>
</AnchorPane>
